<p id="notice"><%= notice %></p>

<h1>Timeline Requests</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>End date</th>
      <th>To</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeline_requests.each do |timeline_request| %>
      <tr>
        <td><%= timeline_request.start_date %></td>
        <td><%= timeline_request.end_date %></td>
        <td><%= timeline_request.to %></td>
        <td><%= link_to 'Show', timeline_request %></td>
        <td><%= link_to 'Edit', edit_timeline_request_path(timeline_request) %></td>
        <td><%= link_to 'Destroy', timeline_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeline Request', new_timeline_request_path %>
